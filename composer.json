{
  "name": "sztyup/multisite",
  "description": "Multisite plugin for Laravel 5",
  "license": "MIT",
  "authors": [{
    "name": "Csupity László",
    "email": "sztyup@gmail.com"
  }],
  "require": {
    "php": ">=7.0",
    "laravel/framework": "^5.5",
    "laravelcollective/html": "^5.5"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Sztyup\\Multisite\\": "src/Multisite"
    },
    "files": [
      "src/helper.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Sztyup\\Multisite\\MultisiteServiceProvider"
      ]
    }
  }
}